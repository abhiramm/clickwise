<!DOCTYPE html>
<html>
<head>
  <title>Clickwise</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
  <%= link_to("Logout", destroy_user_session_path, :method => :delete) %>
<% end %>   
  <p class="notice"><%= notice %></p>
   <p class="alert"><%= alert %></p>

<%= yield %>

</body>
</html>
